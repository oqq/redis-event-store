{
    "name": "prooph/redis-event-store",
    "description": "Prooph Redis EventStore",
    "license": "BSD-3-Clause",
    "homepage": "http://getprooph.org/",
    "authors": [
        {
            "name": "Eric Braun",
            "email": "eb@oqq.be"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-redis": "^3.0",
        "prooph/common": "^4.0",
        "prooph/event-store": "^7.0-beta"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Prooph\\EventStore\\Redis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProophTest\\EventStore\\Redis\\": "tests/",
            "ProophTest\\EventStore\\": "vendor/prooph/event-store/tests/"
        }
    },
    "config": {
        "preferred-install": {
            "prooph/*": "source"
        }
    }
}
